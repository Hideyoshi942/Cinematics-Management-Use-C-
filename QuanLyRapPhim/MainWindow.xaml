<Window x:Class="QuanLyRapPhim.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuanLyRapPhim"
        xmlns:SliderNavigator="clr-namespace:QuanLyRapPhim.View.SliderNavigator"
        
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000" Background="{x:Null}" WindowStartupLocation="CenterScreen" AllowsTransparency="True" WindowStyle="None">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="83*"/>
            <ColumnDefinition Width="167*"/>
        </Grid.ColumnDefinitions>
        <SliderNavigator:SliderNavigatorControl HorizontalAlignment="Left" Foreground="#FFFCFAFA"/>
    </Grid>
</Window>
